package pageObjects;

import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;

public class LoginPage {
	
	//LOGIN PAGE
	
	@FindBy(name = "loginfmt")
	WebElement username;
	
	@FindBy(name = "passwd")
	WebElement password;
	
	@FindBy(id = "idSIButton9")
	WebElement button;//submit and next
	
	@FindBy(id = "idBtn_Back")
	WebElement backbutton;
	
	//NAVIGATION TO PURCHASE ORDER CREATION
	
	@FindBy(xpath = "//*[@id=\\\"mainPane\\\"]/div[5]/div/div/div\")")
	WebElement hambrgricon;
	
	@FindBy(xpath = "//*[@id=\\\"mainPane\\\"]/div[5]/div/div[2]/div/a[1]")
	WebElement Ap;
	

}
